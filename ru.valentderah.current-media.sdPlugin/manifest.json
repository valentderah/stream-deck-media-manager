{
	"Name": "Current Media (Now Playing)",
	"Version": "1.0.0.0",
	"Author": "Valiantsin Dzerakh",
	"Actions": [
		{
			"Name": "Now Playing",
			"UUID": "ru.valentderah.current-media.media-info",
			"Icon": "imgs/actions/media/info",
			"Tooltip": "Displays currently playing media.",
			"PropertyInspectorPath": "ui/media-info.html",
			"Controllers": [
				"Keypad"
			],
			"States": [
				{
					"Image": "imgs/actions/media/info",
					"TitleAlignment": "bottom"
				}
			]
		},
		{
			"Name": "Next Media",
			"UUID": "ru.valentderah.current-media.media-next",
			"Icon": "imgs/actions/media/next",
			"Tooltip": "Skip to next track.",
			"Controllers": [
				"Keypad"
			],
			"States": [
				{
					"Image": "imgs/actions/media/key/next",
					"TitleAlignment": "bottom"
				}
			]
		},
		{
			"Name": "Previous Media",
			"UUID": "ru.valentderah.current-media.media-previous",
			"Icon": "imgs/actions/media/prev",
			"Tooltip": "Skip to previous track.",
			"Controllers": [
				"Keypad"
			],
			"States": [
				{
					"Image": "imgs/actions/media/key/prev",
					"TitleAlignment": "bottom"
				}
			]
		},
		{
			"Name": "Play/Pause",
			"UUID": "ru.valentderah.current-media.media-play-pause",
			"Icon": "imgs/actions/media/play-pause",
			"Tooltip": "Toggle play/pause.",
			"Controllers": [
				"Keypad"
			],
			"States": [
				{
					"Image": "imgs/actions/media/key/play-pause",
					"TitleAlignment": "bottom"
				}
			]
		},
		{
			"Name": "Seek Forward",
			"UUID": "ru.valentderah.current-media.media-forward",
			"Icon": "imgs/actions/media/forward",
